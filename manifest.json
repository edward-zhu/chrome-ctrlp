{
    "name": "Chrome-ctrlp",
    "version": "1.0",
    "description": "Ctrlp extension for Chrome.",
    "permissions": [
        "tabs"
    ],
    "browser_action": {
      "default_title": "Shimarin",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "default": "Ctrl+P",
                "windows": "Ctrl+P",
                "mac": "MacCtrl+P"
            }
        }
    },
    "manifest_version": 2,
    "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
  }